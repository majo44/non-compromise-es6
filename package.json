  "scripts": {
    "start": "node index.js",
    "serve": "run-p serve:*",
    "serve:node": "nodemon index.js",
    "serve:bs": "browser-sync start -p localhost:8008 -w -f app",
    "build": "run-s lint test bundle",
    "test": "karma start karma.config.js",
    "lint": "run-s lint:*",
    "bundle": "webpack",
    "lint:types": "tsc --noEmit",
    "lint:eslint": "eslint --ext .js \"app/src/**/*.js\" \"server/**/*.js\"",
    "format:eslint": "eslint --ext .js \"app/src/**/*.js\" \"server/**/*.js\" --fix",
    "prepare": "run-s webmodules toesm assets",
    "webmodules": "pika-web",
    "toesm": "node tools/prepare/toesm",
    "assets:": "npm-run-all assets:*",
    "assets:es6-promise": "copyfiles -f \"./node_modules/es6-promise/dist/es6-promise.auto.min.js\" \"./app/assets/js/\""
  },
  "browserslist": [
    "ie 11"
  ],
  "@pika/web": {
    "webDependencies": [
      "hardtack",
      "history",
      "lit-element",
      "lit-html"
    ]
  }
